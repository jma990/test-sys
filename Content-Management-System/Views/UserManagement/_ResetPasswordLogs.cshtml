<!-- Reset Password Logs Modal -->
<div class="modal fade" id="resetLogsModal" tabindex="-1" aria-labelledby="resetLogsModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="resetLogsModalLabel">Reset Password Logs</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Admin</th>
              <th>Date</th>
            </tr>
          </thead>
          <tbody id="resetLogsTableBody">
            <tr>
              <td colspan="2" class="text-center text-muted">No logs available</td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
